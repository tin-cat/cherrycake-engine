        "predis/predis": "^1.1",
        "ezyang/htmlpurifier": "^4.10",
        "phpmailer/phpmailer": "6.4.1",
        "webonyx/graphql-php": "^14.7",
        "scssphp/scssphp": "^1.6",
        "yosymfony/toml": "^1.0",
        "ramsey/uuid": "^4.3",
        "aws/aws-sdk-php": "^3.226"
